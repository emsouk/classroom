{% extends 'base.html.twig' %}

{% block title %}Session{% endblock %}

{% block body %}
    <h1>Session</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ session.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ session.name }}</td>
            </tr>
            <tr>
                <th>StartingDate</th>
                <td>{{ session.startingDate ? session.startingDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>EndingDate</th>
                <td>{{ session.endingDate ? session.endingDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ session.level }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ session.createdAt ? session.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ session.updatedAt ? session.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ session.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_session_index') }}">back to list</a>

    <a href="{{ path('app_session_edit', {'id': session.id}) }}">edit</a>

    {{ include('session/_delete_form.html.twig') }}
{% endblock %}
